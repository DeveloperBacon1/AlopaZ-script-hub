-- Prison life


local PF = Window:addPage("Prison life", 5012544693)
local M = PF:addSection("Main")

M:addDropdown("Give gun", {"Remington 870", "AK-47", "M9"}, function(text)
   local A_1 = game:GetService("Workspace")["Prison_ITEMS"].giver[text].ITEMPICKUP
   local Event = game:GetService("Workspace").Remote.ItemHandler
   Event:InvokeServer(A_1)
end)

M:addDropdown("Mod gun", {"Remington 870", "AK-47", "M9"}, function(v)
 module = nil
  if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(v) then
   module = require(game:GetService("Players").LocalPlayer.Backpack[v].GunStates)
    elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild(v) then
   module = require(game:GetService("Players").LocalPlayer.Character[v].GunStates)
  end
 if module ~= nil then
  module["MaxAmmo"] = math.huge
  module["CurrentAmmo"] = math.huge
  module["StoredAmmo"] = math.huge
  module["FireRate"] = 0.000001
  module["Spread"] = 0
  module["AutoFire"] = true
  module["Bullets"] = 10
  module["ReloadTime"] = 0.1
 end
end)

-- Ninja legedns

local PF = Window:addPage("Ninja legends", 5012544693)
local A = PF:addSection("Autofarm")
local T = PF:addSection("Teleport")

A:addToggle("Autoswing (EQUIP Katana)", nil, function(v)
 getgenv().autoswing = v
 while true do
     if not getgenv().autoswing then return end
     local A_1 = "swingKatana"
     local Event = plr.ninjaEvent
     Event:FireServer(A_1)
     wait(0.025)
     end
end)

A:addToggle("Autosell", nil, function(v)
 getgenv().autoswing = v
 while true do
     if not getgenv().autoswing then return end
     game:GetService("Workspace").sellAreaCircles.sellAreaCircle16.circleInner.CFrame = char.HumanoidRootPart.CFrame
     wait(0.1)
     game:GetService("Workspace").sellAreaCircles.sellAreaCircle16.circleInner.CFrame = CFrame.new(0,0,0)
     wait(0.1)
     end
end)

T:addButton("Unlock all islands", function()
  local oldp = char.HumanoidRootPart.CFrame
  for _,v in pairs(game:GetService("Workspace").islandUnlockParts:GetChildren()) do
       char.HumanoidRootPart.CFrame = v.CFrame
       wait(0.1)
      end
end)

-- Tapping legends x


local TLX = Window:addPage("Tapping legends x", 5012544693)
local Ma = TLX:addSection("Main")

Ma:addToggle("AutoTap", nil, function(v)
_G.at = v
while _G.at == true do
     game:GetService("ReplicatedStorage").Remotes.Tap:FireServer()
     wait(0.00000000000000001)
    end
end)

Ma:addToggle("AutoRebirth", nil, function(v)
_G.at = v
while _G.at == true do
local args = {
    [1] = 1
}
game:GetService("ReplicatedStorage").Remotes.Rebirth:FireServer(unpack(args))
    wait(0.000000001)
    end
end)

-- Settings

local S = Window:addPage("Settings", 5012544693)
local Mai = S:addSection("Main")



Mai:addKeybind("Toggle Keybind", Enum.KeyCode.Q, function()
Window:toggle()
end, function()
print("Changed Keybind")
end)
